{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "migrate": "node scripts/validate-migration.js",
    "migrate:validate": "node scripts/validate-migration.js",
    "migrate:legacy": "node scripts/migrate.js",
    "seed": "node scripts/seed-dev-data.js",
    "import": "npx tsx scripts/import-content.ts --data-dir=../../python/final_output --images-dir=../../python/final_output/images",
    "import:dry-run": "npx tsx scripts/import-content.ts --data-dir=../../python/final_output --images-dir=../../python/final_output/images --dry-run",
    "validate-import": "npx tsx scripts/validate-import.ts",
    "validate-import:full": "npx tsx scripts/validate-import.ts --full-report --spot-check=10",
    "rollback-import": "npx tsx scripts/rollback-import.ts --dry-run",
    "rollback-import:confirm": "npx tsx scripts/rollback-import.ts --confirm",
    "db:clear": "npx tsx scripts/clear-database.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "bootstrap": "npm run migrate && npm run seed"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@nclex311/types": "*",
    "@supabase/supabase-js": "^2.57.4",
    "@types/bcrypt": "^6.0.0",
    "@types/pg": "^8.15.5",
    "@vercel/blob": "^2.0.0",
    "@vercel/postgres": "^0.10.0",
    "bcrypt": "^6.0.0",
    "commander": "^14.0.1",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.5",
    "next": "15.5.2",
    "next-auth": "^5.0.0-beta.29",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.4",
    "tsx": "^4.20.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lightningcss": "^1.28.1",
    "node-fetch": "^2.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
